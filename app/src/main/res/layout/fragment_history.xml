<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragement.HistoryFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="499dp"
        android:layout_height="855dp">

        <TextView
            android:id="@+id/textView14"
            android:layout_width="111dp"
            android:layout_height="34dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="80dp"
            android:text="Recent Buy"
            android:textSize="20dp"
            android:fontFamily="@font/bree_serif"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView

            android:id="@+id/imageView2"
            android:layout_width="209dp"
            android:layout_height="139dp"
            android:src="@drawable/momos"
            tools:layout_editor_absoluteX="-10dp"
            tools:layout_editor_absoluteY="128dp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="209dp"
            android:layout_height="143dp"
            android:src="@drawable/burger"
            tools:layout_editor_absoluteX="-10dp"
            tools:layout_editor_absoluteY="281dp" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="211dp"
            android:layout_height="228dp"
            android:src="@drawable/download"
            tools:layout_editor_absoluteX="-9dp"
            tools:layout_editor_absoluteY="397dp" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="MOMO"
            android:fontFamily="@font/bree_serif"
            android:textSize="20dp"
            android:visibility="visible"
            tools:layout_editor_absoluteX="217dp"
            tools:layout_editor_absoluteY="152dp"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="260dp"
            android:fontFamily="@font/bree_serif"
            android:layout_marginBottom="575dp"
            android:textSize="20dp"
            android:text="5$"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/textView16"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Buy Again"
            android:fontFamily="@font/bree_serif"
            tools:layout_editor_absoluteX="325dp"
            tools:layout_editor_absoluteY="186dp" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="BURGER"
            android:textSize="20dp"
            android:fontFamily="@font/bree_serif"
            tools:layout_editor_absoluteX="219dp"
            tools:layout_editor_absoluteY="293dp" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8$"
            android:textSize="20dp"
            android:fontFamily="@font/bree_serif"
            tools:layout_editor_absoluteX="223dp"
            tools:layout_editor_absoluteY="377dp" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:text="SANDWICHES"
            android:textSize="20dp"
            android:fontFamily="@font/bree_serif"
            tools:layout_editor_absoluteX="220dp"
            tools:layout_editor_absoluteY="451dp" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="10$"
            android:textSize="20dp"
            android:fontFamily="@font/bree_serif"
            tools:layout_editor_absoluteX="220dp"
            tools:layout_editor_absoluteY="537dp" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Buy Again"
            android:fontFamily="@font/bree_serif"
            tools:layout_editor_absoluteX="325dp"
            tools:layout_editor_absoluteY="322dp" />

        <Button
            android:id="@+id/button5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Buy Again"
            android:fontFamily="@font/bree_serif"
            tools:layout_editor_absoluteX="325dp"
            tools:layout_editor_absoluteY="483dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible" />

</FrameLayout>